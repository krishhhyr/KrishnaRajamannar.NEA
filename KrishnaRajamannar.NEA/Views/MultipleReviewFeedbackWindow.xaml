<Window x:Class="KrishnaRajamannar.NEA.Views.MultipleReviewFeedbackWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KrishnaRajamannar.NEA.Views"
        mc:Ignorable="d"
        Title="MultipleReviewFeedbackWindow" Height="495" Width="880" 
        ResizeMode="NoResize" FontFamily="Segoe UI Light">

    <Border Background="#7474b0" BorderBrush="AntiqueWhite" BorderThickness="2">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="100"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
            </Grid.ColumnDefinitions>

            <Grid.RowDefinitions>
                <RowDefinition Height="75"></RowDefinition>
                <RowDefinition Height="430"></RowDefinition>
            </Grid.RowDefinitions>

            <Button x:Name="backBtn" Content="Back" Grid.Column="0"
                    FontSize="20" Background="WhiteSmoke" Margin="5" Height="40" Width="75"></Button>
        </Grid>
    </Border>
</Window>
